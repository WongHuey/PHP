<!DOCTYPE html>
<html lang="en">
<head>
<meta charset="utf-8">
<title>{$page_title}</title>
<meta name="Keywords" content="{$keywords}" />
<meta name="Description" content="{$description}" />
<meta name="viewport" id="viewport" content="width=device-width, initial-scale=1, maximum-scale=1">
<link href="/css/master.css" rel="stylesheet">
<link href="/css/header.css" rel="stylesheet">
<!-- SWITCHER -->
<link rel="stylesheet" id="switcher-css" type="text/css" href="/plugins/switcher/css/switcher.css" media="all" />
<!--[if lt IE 9]>
		<script src="js/html5shiv.min.js"></script>
		<script src="js/respond.min.js"></script>
		<![endif]-->
{insert_scripts files='common.js'}
</head>
<body class="animated-css" data-scrolling-animations="false">
<div class="sp-body">
 <!-- Loader Landing Page -->
 <div id="loader">
  <div class="loader"></div>
 </div>
 <!-- Loader end -->
 <!-- #BeginLibraryItem "/library/page_header.lbi" --><!-- #EndLibraryItem -->
 <section id="pageTitleBox" class="paralax breadcrumb-container"  style="background-image: url('/images/contact-heading-bg.jpg');">
  <!-- <div class="overlay"></div> -->
  <div class="container relative">
   <h2 class="title color-main text-capitalize zoomIn" style="color:#333333">KBL Article</h2>
  </div>
 </section>
 <!-- Map -->
 <!-- Map -->
 <section id="contact-us">
  <div class="container">
   <div class="row"> <!-- #BeginLibraryItem "/library/ur_here.lbi" --><!-- #EndLibraryItem -->
    <div class="box" style="line-height:22px">
     <div class="box_1">
      <div class="boxCenterList">
        <ul class="blog-posts">
       <!-- {foreach from=$artciles_list item=article} -->
        <li class="post-item col-lg-18 blogdiv">
         <article class="entry">
          <div class="row">
           <div class="col-sm-12 blogli">
            <h3 class="entry-title"><a href="{$article.url}" title="{$article.title|escape:html}" target="_blank">{$article.short_title}</a></h3>
            <div class="blogdiv2">
             <div class="entry-excerpt blogimg2" style="word-wrap:break-word; word-break:normal; width:100%">{$article.content|truncate:500} <br>
              <a href="{$article.url}" class="button" target="_blank">Read more <i class="fa fa-angle-double-right"></i></a></div>
            </div>
           </div>
          </div>
         </article>
        </li>   
       <!-- {/foreach} -->
       </ul>
      </div>
     </div>
    </div>
    <div class="blank5"></div>
    <!-- #BeginLibraryItem "/library/pages.lbi" --><!-- #EndLibraryItem --> </div>
  </div>
 </section>
 <!-- #BeginLibraryItem "/library/page_footer.lbi" --><!-- #EndLibraryItem --> </div>
</body>
</html>
