<!DOCTYPE html>
<html xmlns="http://www.w3.org/1999/xhtml" xml:lang="en" lang="en">
<head>
<meta charset="utf-8">
<!-- TemplateBeginEditable name="doctitle" -->
<title>{$page_title}</title>
<!-- TemplateEndEditable --><!-- TemplateBeginEditable name="head" --><!-- TemplateEndEditable -->
<meta name="Keywords" content="{$keywords}" />
<meta name="Description" content="{$description}" />
<meta name="googlebot" content="NOODP" />
<meta name="robots" content="INDEX,FOLLOW" />
<meta name="viewport" content="width=device-width, initial-scale=1, maximum-scale=1" />
<meta property="og:locale" content="en_US" />
<meta property="og:type" content="product" />
<meta property="og:title" content="{$page_title}" />
<meta property="og:description" content="{$description}" />
<meta property="og:url" content="{$get_url_cat}" />
<meta property="og:site_name" content="KaBeiLu Hair" />
<meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
<link rel="shortcut icon" href="favicon.ico" />
<link rel="icon" href="animated_favicon.gif" type="image/gif" />
<link href="/fonts/font-awesome/css/font-awesome.min.css" rel="stylesheet">
<link href="/fonts/elegant-icon-font/style.css" rel="stylesheet">
<link href="/css/bootstrap-t.css" rel="stylesheet">
<link href="/css/category-min.css" rel="stylesheet">
		<!-- SWITCHER -->
<link rel="canonical" href="{$get_url_cat}" />
		<!--[if lt IE 9]>
		<script src="js/html5shiv.min.js"></script>
		<script src="js/respond.min.js"></script>
		<![endif]-->
</head>
	<body class="animated-css" data-scrolling-animations="false">
     <div class="sp-body">

      <!-- Loader Landing Page -->
       <div id="loader">
        <div class="loader"></div>  
      </div>
      <!-- Loader end -->
    
    
    <!-- #BeginLibraryItem "/library/page_header.lbi" --><!-- #EndLibraryItem -->
			<section id="pageTitleBox" class="paralax breadcrumb-container2"  style="background-image: url('images/detail-heading-bg.jpg');">
        	<div class="overlay"></div>
				<div class="container relative">
               <span class="zoomIn" style="font-size:12px" data-wow-delay="0.3s"><!-- #BeginLibraryItem "/library/ban2.lbi" --><!-- #EndLibraryItem --></span>				</div>
  </section>
			<section id="pageContent" class="page-content">
				<div class="container">
                      <!-- #BeginLibraryItem "/library/ur_here.lbi" --><!-- #EndLibraryItem -->
					<div class="row"> 
						<div class="sidebar col-lg-3 col-md-3 col-sm-3 col-xs-12 clearfix categorytitle ycmobile">
                 <!-- #BeginLibraryItem "/library/category_tree8.lbi" --> <!-- #EndLibraryItem --></div>      
					  <div class="col-lg-9 col-md-9 col-sm-9 col-xs-12 clearfix2 categorylist">          
						<div class="content-box">
		<div class="isotope-frame">
								<div class="isotope-filter">          
<!--{foreach from=$goods_list item=goods name=goods_list}-->
                                     <!-- {if $goods.goods_id} -->
                <div class="isotope-item newproducts  col-md-4 col-sm-6" data-wow-delay="0.3s">
										<div class="product-item-cat hvr-underline-from-center">
											<div class="product-item_body">
											  <img class="product-item_image" src="{$goods.goods_img}" alt="{$goods.keywords}">
											  <a class="product-item_link" href="{$goods.url}">
										  <span class="product-item_sale color-main font-additional customBgColor circle">{$goods.goods_weight}</span>												</a></div>
					    <a href="{$goods.url}" class="product-item_footer">
												<div class="product-item_title lineh text-center text-capitalize">{$goods.goods_name}</div>
												<div class="product-item_price font-weight-normal customColor">{$goods.shop_price}<span>{$goods.market_price}</span></div>
										  </a></div>
								  </div>
                                     <!--{/if}-->
                                     <!--{/foreach}-->
</div>
              		</div>
 <!-- #BeginLibraryItem "/library/pages.lbi" --><!-- #EndLibraryItem -->  
              <div class="category-pagination col-sm-12">
                   <nav>
                      <ul class="pagination pagination-small pull-right">
                        <li>                     </li>
                      </ul>
                </nav>
                  </div> 
</div>
							</div>
				  </div>
					</div>
	
			</section>

      <!-- #BeginLibraryItem "/library/page_footer.lbi" --><!-- #EndLibraryItem -->
		</div>
	</body>
</html>